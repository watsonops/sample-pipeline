@using Medulla.Editor.Client.Abstractions.ObjectComposition

<div>
    <PropertyList PropertiesMenu="this" PropertyMenuStructure="PropertyMenuStructure"/>
</div>

@code {

    /// <summary>
    /// Forces render of component.
    /// </summary>
    public new void ShouldRender()
    {
        StateHasChanged();
    }

    /// <summary>
    /// The menu structure to be rendered.
    /// </summary>
    [Parameter]
    public PropertyMenuStructure? PropertyMenuStructure { get; set; }

}
